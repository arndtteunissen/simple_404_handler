{
    "name": "arndtteunissen/simple_404_handler",
    "description": "Enables simple 404 Page NotFound handling, also for multilingual sites.",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "error-handling",
        "404",
        "page-not-found"
    ],
    "homepage": "https://github.com/arndtteunissen/simple_404_handler",
    "support": {
        "issues": "https://github.com/arndtteunissen/simple_404_handler/issues"
    },
    "authors": [
        {
            "name": "Joshua Westerheide",
            "email": "dev@jdoubleu.de"
        },
        {
            "name": "Tim Schreiner",
            "email": "schreiner.tim@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "typo3/cms-frontend": "^6.2 || ^7.6 || ^8.7"
    },
    "autoload": {
        "psr-4": {
            "Arndtteunissen\\Simple404Handler\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
